<% provide(:title,@testpaper.title)%>
<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="single-panel-header">
      <h3><%=@testpaper.title%></h3>
    </div>
    <% (1..3).each do |idx|%>
      <div class="question-panel-header">
        <h3>
          <%=number_to_hanzi(idx)%>„ÄÅ<%=get_qst_type_name(idx)%>
        </h3>
      </div>
      <div id="pnl_qsttype_<%=idx%>">
        <% if !@hashques[idx].nil? %>
          <% @hashques[idx].each_with_index do |question,index|%>
            <%=render partial: "do"+get_qst_partial_name(idx),locals: {question: question,idx: index+1}%>

            <% end %>
        <% end %>

      </div>
    <% end %>
  </div>
</div>
